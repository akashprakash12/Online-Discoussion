<div class="container">
    <div class="row">
        <div class="col-12 col-md-4 col-lg-2">
            <h2>Reported</h2>

            <div
                class="avathar-Answer rounded-pill ml-auto mr-auto mt-3 text-center position-relative"
            >
                <img src="profile/{{user.UserAnswer._id}}.jpg" class="rounded-pill" alt="" />
                <div class="icon position-absolute">
                    <span>{{user.report}}</span>
                    <span
                        class="iconify"
                        data-inline="false"
                        data-icon="octicon:alert-16"
                        style="color: #7e0a0a; font-size: 30px"
                    ></span>

                </div>
                <h4>{{user.UserAnswer.Username}}</h4>
            </div>
        </div>
        <div class="col-12 col-md-8 col-lg-10">
            <div class="Answer-container badge-secondary p-4 rounded my-5">
                <p>
                    {{user.Answercode.answer}}
                </p>

                <div class="other-info mt-3 mb-3">
                    <img src="codeimg/{{user._id}}.jpg" alt="" />
                </div>
                <div class="code pt-5 p-3">
                    <div class="copy">copy</div>
                    <p>
                        {{user.Answercode.answercode}}
                    </p>
                </div>
                <div class="video mt-3">
                    <video src="codevideo/{{user._id}}.mp4" controls></video>
                </div>
            </div>
        </div>
    </div>
    <span class="d-block" style="width: 100%; height: 1px; background-color: black"></span>
    <div id="drop-like">
        <span
            class="iconify"
            data-inline="false"
            data-icon="fe:drop-down"
            style="font-size: 24px"
        ></span>
    </div>

    <div class="move" id="Liked">
        <h2>Liked</h2>
        <div class="row">
            <div class="col-12 col-md-4 col-lg-2">
                <div
                    class="avathar-Answer rounded-pill ml-auto mr-auto mt-3 text-center position-relative"
